<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".crear_equipo">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/a_392"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="325dp"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/a_80"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="225dp"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/truemas"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="125dp"/>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/a_9"
        android:layout_marginLeft="225dp"
        android:layout_marginTop="325dp"/>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/a_3"
        android:layout_marginLeft="225dp"
        android:layout_marginTop="225dp"/>

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/a_6"
        android:layout_marginLeft="225dp"
        android:layout_marginTop="125dp"/>

    <EditText
        android:id="@+id/textView"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:hint="Nombre del Equipo"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="25dp"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floating"
        android:layout_width="275dp"
        android:layout_height="275dp"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="450dp"
        android:src="@drawable/tick"/>
</RelativeLayout>